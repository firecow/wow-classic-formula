{
  "require": {
    "twig/twig": "2.5.*",
    "league/route": "4.2.*",
    "league/climate": "3.4.*",
    "guzzlehttp/psr7": "1.4.*",
    "guzzlehttp/guzzle": "6.3.*",
    "guzzlehttp/promises": "1.3.*",
    "phpunit/phpunit": "7.4.*",
    "firebase/php-jwt": "5.0.*",
    "monolog/monolog": "1.24.*",
    "php": "7.2.*",
    "ext-json": "*",
    "ext-PDO": "*",
    "ext-libxml": "*",
    "ext-dom": "*"
  },
  "require-dev": {
    "symplify/easy-coding-standard": "5.2.*",
    "phpstan/phpstan": "0.10.*",
    "phpstan/phpstan-strict-rules": "0.10.*"
  },
  "autoload": {
    "psr-4": {
      "Routes\\": "routes/",
      "App\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
